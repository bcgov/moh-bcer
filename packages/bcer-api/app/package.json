{
  "name": "bcer-api",
  "version": "0.0.1",
  "description": "Vaping regulations for BC",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "typeorm": "typeorm-ts-node-commonjs",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist",
    "build:pipeline": "set NODE_ENV=production && tsc -p tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist && copy \".env.build\" \"dist/.env\" && copy \"package.json\" \"dist/package.json\"",
    "build:prem": "cross-env NODE_ENV=production rm -rf ./dist && tsc -p tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist && cp .env.build dist/.env && cp package.json dist/package.json",
    "build-nest": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=local nodemon --config nodemon.json",
    "start:boilerplate": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nest start --watch",
    "dev": "nodemon --config nodemon.json",
    "migrate:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource ./datasource.js migration:create",
    "migrate:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource ./datasource.js migration:run",
    "migrate:start": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run --dataSource ./datasource.js && node dist/main",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "tar": "tar -czvf ../dist.tar.gz ./dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sonar": "node src/sonar.ts",
    "typedoc": "typedoc",
    "geo-code-existing-address": "node scripts/geo-code-existing-address.js"
  },
  "dependencies": {
    "@babel/traverse": "7.25.3",
    "@googlemaps/google-maps-services-js": "3.4.0",
    "@nestjs/common": "10.3.10",
    "@nestjs/core": "10.3.10",
    "@nestjs/platform-express": "10.3.10",
    "@nestjs/schedule": "4.1.0",
    "@nestjs/swagger": "7.4.0",
    "@nestjs/typeorm": "10.0.2",
    "@nestjsx/crud": "5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "5.0.0-alpha.3",
    "axios": "1.7.3",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "core-js": "^3.38.0",
    "crypto": "1.0.1",
    "crypto-js": "4.2.0",
    "csvtojson": "2.0.10",
    "dotenv": "16.4.5",
    "geojson-utils": "1.1.0",
    "https-proxy-agent": "7.0.5",
    "jsonwebtoken": "9.0.2",
    "jszip": "3.10.1",
    "jwks-rsa": "3.1.0",
    "keycloak-connect": "25.0.2",
    "keycloak-js": "25.0.2",
    "moment": "2.30.1",
    "nest-csv-parser": "2.0.4",
    "nest-keycloak-connect": "1.10.1",
    "nest-winston": "1.9.7",
    "notifications-node-client": "8.2.1",
    "pg": "8.12.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.1",
    "swagger-ui-express": "5.0.1",
    "typeorm": "0.3.20",
    "winston": "3.13.1",
    "winston-daily-rotate-file": "5.0.0",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@nestjs/cli": "10.4.2",
    "@nestjs/schematics": "10.1.3",
    "@nestjs/testing": "10.3.10",
    "@types/babel__core": "7.20.5",
    "@types/babel__traverse": "7.20.6",
    "@types/bcryptjs": "2.4.6",
    "@types/cron": "2.4.0",
    "@types/estree": "1.0.5",
    "@types/express": "4.17.21",
    "@types/faker": "6.6.9",
    "@types/jest": "29.5.12",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "22.1.0",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "8.0.1",
    "@typescript-eslint/parser": "8.0.1",
    "cross-env": "7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "jest": "29.7.0",
    "node-mocks-http": "1.15.1",
    "nodemon": "3.1.4",
    "prettier": "3.3.3",
    "sonarqube-scanner": "4.2.0",
    "sqlite3": "5.1.7",
    "supertest": "7.0.0",
    "ts-jest": "29.2.4",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "tsc-alias": "1.8.10",
    "tsconfig-paths": "4.2.0",
    "tscpaths": "0.0.9",
    "typedoc": "0.26.5",
    "typescript": "5.5.4"
  }
}
