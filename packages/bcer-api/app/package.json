{
  "name": "bcer-api",
  "version": "0.0.1",
  "description": "Vaping regulations for BC",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist",
    "build:aws": "rm -rf ./dist && tsc -p tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist",
    "build:pipeline": "set NODE_ENV=production && tsc -p tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist && copy \"ormconfig.js\" \"dist/ormconfig.js\" && copy \".env.build\" \"dist/.env\" && copy \"package.json\" \"dist/package.json\"",
    "build:prem": "cross-env NODE_ENV=production rm -rf ./dist && tsc -p tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist && cp ormconfig.js dist/ormconfig.js && cp .env.build dist/.env && cp package.json dist/package.json",
    "build-nest": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=local nodemon --config nodemon.json",
    "start:boilerplate": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nest start --watch",
    "dev": "nodemon --config nodemon.json",
    "migrate:create": "typeorm migration:generate",
    "migrate:run": "typeorm migration:run",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "tar": "tar -czvf ../dist.tar.gz ./dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sonar": "node src/sonar.ts",
    "typedoc": "typedoc"
  },
  "dependencies": {
    "@nestjs/common": "7.0.0",
    "@nestjs/core": "7.0.0",
    "@nestjs/platform-express": "7.0.0",
    "@nestjs/schedule": "1.0.1",
    "@nestjs/swagger": "4.5.12",
    "@nestjs/typeorm": "7.1.5",
    "@nestjsx/crud": "4.6.2",
    "@nestjsx/crud-typeorm": "4.6.2",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "crypto": "1.0.1",
    "csvtojson": "2.0.10",
    "dotenv": "8.2.0",
    "faker": "4.1.0",
    "jsonwebtoken": "8.5.1",
    "jszip": "3.5.0",
    "jwks-rsa": "1.10.1",
    "keycloak-connect": "11.0.0",
    "keycloak-js": "11.0.0",
    "moment": "2.29.1",
    "nest-csv-parser": "2.0.4",
    "nest-keycloak-connect": "1.2.1",
    "nest-winston": "1.4.0",
    "pg": "8.3.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.2",
    "swagger-ui-express": "4.1.4",
    "typeorm": "0.2.31",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.0",
    "zlib": "1.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "7.0.0",
    "@nestjs/schematics": "7.0.0",
    "@nestjs/testing": "7.0.0",
    "@types/bcryptjs": "2.4.2",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.3",
    "@types/faker": "4.1.12",
    "@types/jest": "25.2.3",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "13.9.1",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "cross-env": "7.0.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "jest": "26.0.1",
    "node-mocks-http": "1.9.0",
    "nodemon": "2.0.4",
    "prettier": "1.19.1",
    "sonarqube-scanner": "2.7.0",
    "sqlite3": "5.0.0",
    "supertest": "4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "tscpaths": "0.0.9",
    "typedoc": "0.18.0",
    "typescript": "3.9.7"
  }
}
