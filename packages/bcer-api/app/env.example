# Optional entry, default: '0 1 16 1 *'
CLOSE_LOCATION_CRON_TIME=<Cron Expression>

# Optional entry, default: '12-01'
NOI_EXPIRY_DATE=<MM-DD>

# Required for cronjob, value is a csv of cronJob names "CLOSE_LOCATION_WITH_EXPIRED_NOI"
# Only include the names of cron you wish to run
CRON_JOB_NAMES=<Cron job names in csv format>

# Optional entry, default: 'America/Vancouver'
CRON_TIME_ZONE=<Time Zone>

#--------------------------- Text Notifications ------------------------#
# Optional entry, default: https://api.notification.canada.ca
TEXT_API_ENDPOINT=<Api endpoint to text>

# Required entry
TEXT_GENERIC_NOTIFICATION_TEMPLATE_ID=<Template id>

# Required entry
TEXT_API_KEY=<Text service api key>

#Optional entry, default: abc123
TEXT_REFERENCE=<text reference>

# To Enable text notification and subscription
ENABLE_TEXT_MESSAGES=true
ENABLE_SUBSCRIPTION=true

#--------------------------- Map ------------------------------------#
# Required
BC_DIRECTION_API_KEY=<api_key>

#Required
MAP_BOX_ACCESS_TOKEN=<access_token>

#--------------------------------------------------------------------#

